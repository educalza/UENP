<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="270.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.UsuarioLogin">
   <children>
      <VBox layoutX="56.0" layoutY="66.0" prefHeight="122.0" prefWidth="290.0" spacing="15.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <TextField fx:id="txtLogin" promptText="Login" />
            <TextField fx:id="txtSenha" promptText="Senha" />
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button fx:id="btnEntrar" onAction="#entrar" text="Entrar" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Label layoutX="148.0" layoutY="20.0" style="-fx-font-size: 16; -fx-font-weight: bold;" text="Login Usuario" />
      <Button fx:id="btnCadastrar" layoutX="168.0" layoutY="231.0" onAction="#cadastrar" text="Cadastrar" />
      <Label layoutX="159.0" layoutY="202.0" text="Fazer Cadastro">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
